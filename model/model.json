{
  "sys": {
    "shape": {
      "srv": {}
    }
  },
  "main": {
    "srv": {
      "monitor": {
        "msg": {},
        "plugin": {
          "repl": {}
        }
      },
      "web": {
        "msg": {}
      },
      "info": {
        "msg": {
          "role:info,collect:part": {
            "async": true
          },
          "role:info,get:info": {
            "params": {}
          },
          "role:info,test:true": {}
        }
      },
      "update": {
        "msg": {
          "role:update,cmd:download_registry": {},
          "role:update,cmd:stop_registry_download": {},
          "role:update,cmd:follow_registry": {},
          "role:update,cmd:stop_following_registry": {},
          "role:update,cmd:process_change": {},
          "role:update,cmd:start": {},
          "role:update,cmd:stop": {}
        }
      },
      "npm": {
        "msg": {
          "role:info,need:part": {
            "async": true
          },
          "role:source,source:npm,get:package": {},
          "role:source,source:npm,pull:package": {}
        }
      },
      "github": {
        "msg": {
          "role:info,need:part": {
            "async": true
          },
          "role:source,source:github,get:package": {},
          "role:source,source:github,pull:package": {}
        }
      },
      "search": {
        "msg": {
          "role:search,search:query": {}
        }
      }
    },
    "ent": {
      "nodezoo": {
        "search": {
          "term": {
            "type": "N"
          },
          "when": {
            "type": "N"
          }
        },
        "github": {
          "name": {
            "type": "S"
          },
          "owner": {
            "type": "S"
          },
          "repo": {
            "type": "S"
          },
          "stars": {
            "type": "N"
          },
          "forks": {
            "type": "N"
          },
          "last": {
            "type": "N"
          }
        },
        "npm": {
          "name": {
            "type": "S"
          },
          "version": {
            "type": "S"
          },
          "giturl": {
            "type": "S"
          },
          "desc": {
            "type": "S"
          },
          "readme": {
            "type": "S"
          },
          "original_doc": {
            "type": "S"
          }
        }
      }
    },
    "api": {
      "http": {
        "prefix": "/api",
        "route": {
          "query": {
            "path": "/query",
            "msg": {
              "role": "search",
              "cmd": "query",
              "query": "req.query.q"
            }
          }
        }
      }
    },
    "theme": {
      "light": {
        "color": {
          "bg0": "FFFFFF",
          "bg1": "CCCCCC",
          "fg0": "333333",
          "fg1": "000000"
        }
      },
      "dark": {
        "color": {
          "bg0": "000000",
          "bg1": "333333",
          "fg0": "CCCCCC",
          "fg1": "FFFFFF"
        }
      }
    },
    "app": {
      "web": {
        "parts": {
          "head": {},
          "main": {}
        },
        "view": {
          "intro": {},
          "result": {},
          "info": {}
        },
        "route": {
          "home": {
            "path": "/",
            "view": "intro"
          },
          "package": {
            "path": "/info/:package",
            "view": "info"
          }
        }
      }
    }
  }
}